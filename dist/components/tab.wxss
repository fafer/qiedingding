.city-tab-wrap{text-align:center;margin-top:33rpx;margin-bottom:16rpx}.city-tab-wrap .city-tab{border-top:2rpx solid #FF552E;border-bottom:2rpx solid #FF552E;border-left:2rpx solid #FF552E;border-right:2rpx solid #FF552E;border-radius:8rpx;height:66rpx;display:inline-block}.city-tab-wrap .city-tab text{width:162rpx;line-height:66rpx;box-sizing:border-box;display:inline-block}.city-tab-wrap .city-tab text:nth-child(2){border-left:2rpx solid #FF552E;border-right:2rpx solid #FF552E}.city-tab-wrap .city-tab text.active{background-color:#FF552E;color:#ffffff}